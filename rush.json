{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.48.0",
  "pnpmVersion": "6.10.0",
  "pnpmOptions": {
    "pnpmStore": "local",
    "strictPeerDependencies": false,
    "resolutionStrategy": "fewer-dependencies",
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0 || >=16.4.2 <17.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "allowMostlyStandardPackageNames": false,
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "production",
      "tools",
      "prototypes"
    ]
  },
  "gitPolicy": {
    "versionBumpCommitMessage": "chore: applying package updates. [skip-ci]",
    "changeLogUpdateCommitMessage": "docs: updating change logs for package updates. [skip-ci]"
  },
  "repository": {
    "url": "https://github.com/catchingaxes/noetic",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [ ],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": true,
  "hotfixChangeEnabled": false,
  "projects": [
    {
      "packageName": "@noetic/poc-probot",
      "projectFolder": "pocs/node/probot",
      "reviewCategory": "prototypes",
      "shouldPublish": false
    }
  ]
}
